<% include ../partials/header %>


	<%  if (!isAuthenticated){ %>
		<br>
		<div class="alert alert-warning">
		<a href="/login">You must be logged in to view this page</a>
		</div>
	<%  } else {%>

	<h1>Hello, <%= user.username %> - here's your list of parties!</h1>

	<% event_users.forEach(function(event) {%>	
			
				<p>
			
			<%=event.dataValues.title%>
			</p>
			
<%})%>

<%  }%>


	<a href="/logout" class = "btn btn-danger btn-lg">Log out here</a>

	



<% include ../partials/footer %>